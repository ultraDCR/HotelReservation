<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Retrieve</servlet-name>
        <servlet-class>Retrieve</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminRegister</servlet-name>
        <servlet-class>AdminRegister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Confirm</servlet-name>
        <servlet-class>Confirm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayHotel</servlet-name>
        <servlet-class>DisplayHotel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Hotel</servlet-name>
        <servlet-class>Hotel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayBooking</servlet-name>
        <servlet-class>DisplayBooking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayClientuser</servlet-name>
        <servlet-class>DisplayClientuser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayHoteluser</servlet-name>
        <servlet-class>DisplayHoteluser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Retrieve</servlet-name>
        <url-pattern>/retrieve</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminRegister</servlet-name>
        <url-pattern>/adminregister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Confirm</servlet-name>
        <url-pattern>/confirm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayHotel</servlet-name>
        <url-pattern>/displayhotel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Hotel</servlet-name>
        <url-pattern>/hotel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayBooking</servlet-name>
        <url-pattern>/displaybooking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayClientuser</servlet-name>
        <url-pattern>/displayclientuser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayHoteluser</servlet-name>
        <url-pattern>/DisplayHoteluser</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
